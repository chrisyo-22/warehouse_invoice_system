{
  "name": "jinjinfood",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "deno task dev:api & deno task dev:vite",
    "dev:api": "deno run --watch --allow-env --allow-net --allow-write --allow-read api/main.ts",
    "dev:vite": "deno run -A npm:vite",
